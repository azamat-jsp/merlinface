{
    "name": "project/merlinface",
    "type": "project",
    "description": "MerlinFace",
    "keywords": ["MerlinFace", "project"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "symfony/routing": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/mime": "^6.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "config": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
